{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev:root",
  "installCommand": "npm run install:all",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/billing/:path*",
      "destination": "https://YOUR-BILLING-APP.vercel.app/:path*"
    },
    {
      "source": "/connect/:path*",
      "destination": "https://YOUR-CONNECT-APP.vercel.app/:path*"
    }
  ]
}

